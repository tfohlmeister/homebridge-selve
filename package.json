{
  "name": "homebridge-selve",
  "version": "1.0.0",
  "description": "Selve Remote Shutter Support for Homebridge: https://github.com/homebridge/homebridge",
  "license": "MIT",
  "keywords": [
    "homebridge-plugin",
    "selve",
    "shutter",
    "rollershutter",
    "homebridge"
  ],
  "author": "Thorben Fohlmeister <thorben@fohlmeister.com>",
  "repository": {
    "type": "git",
    "url": "git://github.com/tfohlmeister/homebridge-selve.git"
  },
  "bugs": {
    "url": "http://github.com/tfohlmeister/homebridge-selve/issues"
  },
  "engines": {
    "homebridge": ">=1.0.0"
  },
  "main": "dist/index.js",
  "files": [
    "LICENSE",
    "dist",
    "config.schema.json"
  ],
  "dependencies": {
    "fast-xml-parser": "^3.16.0",
    "queue": "^6.0.1",
    "serialport": "^7.1.5",
    "xmldoc": "^1.1.2"
  },
  "devDependencies": {
    "@types/node": "^13.13.4",
    "@types/serialport": "^8.0.0",
    "concurrently": "^5.2.0",
    "homebridge": "^1.0.0",
    "nodemon": "^2.0.3",
    "rimraf": "^3.0.2",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon dist/**/*.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "build": "npm run clean && tsc",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "npx --node-arg=--inspect-brk homebridge -D",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "prepublishOnly": "npm run clean && npm run build",
    "postpublish": "npm run clean"
  }
}
